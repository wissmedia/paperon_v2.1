<!DOCTYPE html>
<html lang="en">
<!-- HEAD PARTIALS -->
<%- include('../partials/head.ejs') %>

  <body>
    <!-- NAV PARTIALS -->
    <%- include('../partials/nav.ejs') %>
      <div class="container">
        <div class="row-content">
          <!-- User Control Menus -->
          <div class="content fadeInKiri">
            <div class="line"></div>
            <div class="bungkus-content">
              <h1>User Control</h1>
            </div>
            <div class="card-container scroll-samping">
              <!-- Menu Item Card -->
              <% UserMenu.forEach(menu=> { %>
                <div class="card <%=(menu.status ? 'pending' : '' ) %>">
                  <a href="<%= menu.link %> ">
                    <h3>
                      <i class="<%= menu.icon %> "></i>
                    </h3>
                    <div class="card-label">
                      <p>
                        <%= menu.label %>
                      </p>
                    </div>
                  </a>
                </div>
                <% }) %>
            </div>
          </div>
          <!-- User Control Menus END -->

          <!-- QForm Control -->
